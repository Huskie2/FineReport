<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[MYSQL_共享数据平台]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select 
distinct(date_format(es_happen_date,'%Y%m'))     as '事件发生日期'

from t_sec_event_survy_history
order by date_format(es_happen_date,'%Y%m') desc]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
