<TableData name="历史事件明细" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="rq"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[MYSQL_共享数据平台]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select 
date_format(es_happen_date,'%Y/%m/%d')     as '事件发生日期'
,es_desc            as '事件描述'
,es_measure         as '处理情况描述'
,es_qualative_cn    as '事件定性'
,es_event_type_cn   as '事件原因'
,es_reason_type_cn  as '事件类型'
,report_vulnerable  as '是否报局方'   
,es_fix_duty_cn     as '责任部门'
,alert_type         as '警告类型'
,report_number     as '主动报告编号'
,event_survey      as '有无事件调查'
,es_remark         as '备注'

from t_sec_event_survy_history
where 1=1  ${if(len(rq) == 0,"","and date_format(es_happen_date,'%Y%m') in ('" + rq + "')")}
order by date_format(es_happen_date,'%Y/%m/%d') desc
]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="参数查询" class="com.fr.data.impl.DBTableData">
